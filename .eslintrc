{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "consistent-return": "off",
        "no-restricted-syntax": [
            "error",
            "FunctionExpression",
            "WithStatement"
        ]
    }
}
